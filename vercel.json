{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "frontend/dist" }
    },
    {
      "src": "api/chunks.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/chunks",
      "dest": "/api/chunks.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    }
  ]
}
